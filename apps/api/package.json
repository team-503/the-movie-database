        "class-transformer": "0.5.1",
        "class-validator": "0.14.1",
        "config": "3.3.12",
        "cookie-parser": "1.4.6",
        "cross-env": "^7.0.3",
        "csurf": "1.11.0",
        "graphql": "16.10.0",
        "helmet": "7.1.0",
        "humps": "2.0.1",
        "lodash": "4.17.21",
        "ms": "2.1.3",
        "passport": "0.7.0",
        "passport-jwt": "4.0.1",
        "reflect-metadata": "0.1.13",
        "rxjs": "7.8.1",
        "sqlite3": "5.1.7",
        "ts-patch": "3.2.1",
        "tsc-alias": "1.8.10",
        "typeorm": "0.3.20",
        "typescript-transform-paths": "3.5.2",
