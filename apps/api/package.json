        "@nestjs/common": "10.0.0",
        "@nestjs/core": "10.0.0",
        "@nestjs/graphql": "12.2.0",
        "@nestjs/jwt": "10.2.0",
        "@nestjs/passport": "10.0.3",
        "@nestjs/platform-express": "10.0.0",
        "@nestjs/terminus": "10.2.3",
        "@nestjs/throttler": "6.2.1",
        "@nestjs/typeorm": "10.0.2",
        "@repo/pkg-env": "*",
        "@repo/pkg-helpers": "*",
        "@repo/pkg-logger": "*",
        "axios": "1.7.7",
        "bcrypt": "5.1.1",
        "body-parser": "1.20.3",
