        "@types/jest": "29.5.2",
        "@types/lodash": "4.17.7",
        "@types/ms": "0.7.34",
        "@types/node": "20.3.1",
        "@types/passport": "1.0.16",
        "@types/passport-jwt": "4.0.1",
        "@types/source-map-support": "0.5.10",
        "lockfile-lint": "4.14.0",
        "source-map-support": "0.5.21",
        "ts-jest": "29.1.0",
        "ts-loader": "9.4.3",
        "ts-node": "10.9.1",
        "tsc": "2.0.4",
